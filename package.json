{
  "name": "prism2colorvariables",
  "description": "Migrate your Prism named color to Color Variables",
  "version": "0.1.1-0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Prism2ColorVariables",
    "manifest": "src/manifest.json",
    "main": "prism2colorvariables.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "repository": "https://github.com/griffin-stewie/Prism2ColorVariables.git",
  "author": "griffin-stewie"
}
